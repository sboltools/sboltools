{
  "name": "sboltools",
  "version": "1.0.0",
  "main": "./dist/src/main.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "./node_modules/typescript/bin/tsc -d -p . --outDir dist",
    "watch": "./node_modules/typescript/bin/tsc --watch -d -p . --outDir dist",
    "package": "./node_modules/webpack/bin/webpack.js",
    "makedist": "node ./makedist.js",
    "test": "./node_modules/typescript/bin/tsc -d -p . --outDir dist && NODE_PATH=dist node dist/test/run-tests.js"
  },
  "dependencies": {
    "@types/chalk": "^2.2.0",
    "@types/node": "^14.0.13",
    "@types/yargs": "^15.0.4",
    "bioterms": "^0.25.0",
    "chalk": "^4.0.0",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "json-colorizer": "^2.2.2",
    "node-fetch": "^2.6.1",
    "promise-fs": "^2.1.1",
    "rdfoo": "^0.12.0",
    "sbolgraph": "^0.45.0",
    "string-argv": "^0.3.1"
  },
  "preferGlobal": true,
  "bin": {
    "sbol": "./bin/dev/sbol"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.1",
    "on-build-webpack": "^0.1.0",
    "source-map-loader": "^0.2.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }

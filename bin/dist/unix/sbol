/***/ "../rdfoo/node_modules/indexof/index.js":
/*!**********************************************!*\
  !*** ../rdfoo/node_modules/indexof/index.js ***!
  \**********************************************/
/*! no static exports found */
/***/ (function(module, exports) {


var indexOf = [].indexOf;

module.exports = function(arr, obj){
  if (indexOf) return arr.indexOf(obj);
  for (var i = 0; i < arr.length; ++i) {
    if (arr[i] === obj) return i;
  }
  return -1;
};


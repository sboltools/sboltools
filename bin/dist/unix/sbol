const SBOL2GraphView_1 = __webpack_require__(/*! ../../SBOL2GraphView */ "../sbolgraph/dist/SBOL2GraphView.js");
const SBOL3GraphView_1 = __webpack_require__(/*! ../../SBOL3GraphView */ "../sbolgraph/dist/SBOL3GraphView.js");

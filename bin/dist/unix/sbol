            throw new Error('one of s/p/o were undefined');
        if (!s && !o) {
            console.dir({ s, p, o });
            throw new Error('matchOne with only a predicate?');
        }
        if (!p && !o) {
            console.dir({ s, p, o });
            throw new Error('matchOne with only a subject?');
